{
  "name": "test-driven-architecture",
  "version": "1.0.0",
  "description": "This package generates architectural tests from solution definitions",
  "main": "index.js",
  "files": [
    "dist/"
  ],
  "watch": {
    "w": {
      "patterns": [
        "src",
        "solution",
        "test"
      ],
      "extensions": "ts,json"
    }
  },
  "scripts": {
    "lint": "# tslint \"src/**/*.ts\"",
    "typings": "typings",
    "w": "rm -rf dist && clear && tsc && DEBUG=example node dist/index.js -s ./solution/solution1.json -e ./solution/environments.json -o build/summary.html ",
    "watch": "npm-watch"
  },
  "keywords": [
    "architecture",
    "test"
  ],
  "author": "Phil Tomlinson",
  "devDependencies": {
    "typescript": "^1.8.9",
    "typings": "^0.7.9"
  },
  "dependencies": {
    "any-promise": "^1.3.0",
    "bluebird": "^3.4.6",
    "camel-case": "^1.2.2",
    "change-case": "^2.3.1",
    "debug": "^2.2.0",
    "handlebars": "^4.0.5",
    "param-case": "^1.1.2",
    "pascal-case": "^1.1.2",
    "yargs": "^4.8.1"
  }
}
