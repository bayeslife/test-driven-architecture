<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <title>{{raml.title}}</title>

<style>
    {{> bootstrap.css}}
 </style>
 <script type="text/javascript">
   {{> jquery.hbs}}
 </script>
 <script type="text/javascript">
     {{> bootstrap.js}}
  </script>

 <style>
     {{> main.css}}
  </style>


<script>
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');


  ga('create', 'UA-76789851-1', 'auto', {
        'allowLinker': true,
        'storage': 'none','clientId':localStorage.getItem('gaClientId')
    });

  ga('set', 'checkProtocolTask', null);
  ga('set','page','/{{pascalCase solution.title}}');
  ga('send', 'pageview');

</script>

</head>
<body>
  <div class="container">

  <h1>Infrastructure View of solution</h1>

<h2>Implementation Tests</h2>
  <table>
    <tr>
      <th>name</th>
      <th>component</th>
      <th>environment</th>
      <th>zone</th>
      <th>host</th>
      <th>test</th>
    </tr>
    {{#each implementationTests}}
      <tr>
        <td>{{name}}</td>
        <td>{{component}}</td>
        <td>{{environment}}</td>
        <td>{{zone}}</td>
        <td>{{host}}</td>
        <td>{{test}}</td>
      </tr>
    {{/each}}
</table>


</div>

</body>

</html>
